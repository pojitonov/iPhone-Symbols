t.GetObjectByName("label4").pathAttributes.fillColor = "#324D83";}  if(active == "Five"){Widget.GetObjectByName("label5").pathAttributes.fillColor = "#324D83";}}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}