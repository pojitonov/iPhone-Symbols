//==================================================================// iPhone Thread Count// Timur Prokopiev 2011//==================================================================function setDefaultValues() {  var values = new Array();  // Count  values.push({ name:"Count", type:"text", value:"3"});Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  Widget.GetObjectByName("count").textChars = values[0].value;}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}