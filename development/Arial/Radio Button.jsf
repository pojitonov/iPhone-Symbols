//==================================================================// iPhone Radio Button// Timur Prokopiev 2011//==================================================================function setDefaultValues() {  var values = new Array();  // Enable Radio Button  values.push({ name:"Enable", type:"Boolean",  value:"true" });Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var enabled = (values[0].value.toString()=="false");		  Widget.GetObjectByName("radio button (selected)").visible = (!enabled);  Widget.GetObjectByName("radio button (unselected)").visible = (enabled);	}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}