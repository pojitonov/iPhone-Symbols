//==================================================================// iPhone Sort Buttons (2 Blue)// Timur Prokopiev 2011//==================================================================function setDefaultValues() {  var values = new Array();// Active  values.push({ name:"Active", type:"ComboBox", value:"None,None,First,Second"});// First Text  values.push({ name:"First Text", type:"text", value:"Label"});// Second Text  values.push({ name:"Second Text", type:"text", value:"Label"});Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var state  = values[0].value.split(",")[0].toString();  Widget.GetObjectByName("divider (inactive)").visible = false;  Widget.GetObjectByName("button2 (inactive)").visible = false;  Widget.GetObjectByName("button1 (inactive)").visible = false;  Widget.GetObjectByName("label1").textChars = values[1].value;  Widget.GetObjectByName("label2").textChars = values[2].value;  Widget.GetObjectByName("label1").visible = true;  Widget.GetObjectByName("label2").visible = true;if (state == "None") {  Widget.GetObjectByName("divider (inactive)").visible = true;  Widget.GetObjectByName("divider (active)").visible = false;  Widget.GetObjectByName("button1 (active)").visible = false;  Widget.GetObjectByName("button2 (active)").visible = false;  Widget.GetObjectByName("button1 (inactive)").visible = true;  Widget.GetObjectByName("button2 (inactive)").visible = true;		}else if (state == "First") {  Widget.GetObjectByName("divider (inactive)").visible = false;  Widget.GetObjectByName("divider (active)").visible = true;  Widget.GetObjectByName("button1 (active)").visible = true;  Widget.GetObjectByName("button2 (active)").visible = false;  Widget.GetObjectByName("button1 (inactive)").visible = false;  Widget.GetObjectByName("button2 (inactive)").visible = true;		}else if (state == "Second") {  Widget.GetObjectByName("divider (inactive)").visible = false;  Widget.GetObjectByName("divider (active)").visible = true;  Widget.GetObjectByName("button1 (active)").visible = false;  Widget.GetObjectByName("button2 (active)").visible = true;  Widget.GetObjectByName("button1 (inactive)").visible = true;  Widget.GetObjectByName("button2 (inactive)").visible = false;		}}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}