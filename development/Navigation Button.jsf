//==================================================================// iPhone Navigation Button// Timur Prokopiev 2011//==================================================================function setDefaultValues() {  var values = new Array();  // Button Text  values.push({ name:"Text", type:"text", value:"Label"});// Button Style  values.push({ name:"Style", type:"ComboBox", value:"Button,Button,Left,Right"});Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var style  = values[1].value.split(",")[0].toString();  Widget.GetObjectByName("right_label").visible = (style == "Right");  Widget.GetObjectByName("right_bg").visible = (style == "Right");  Widget.GetObjectByName("left_label").visible = (style == "Left");  Widget.GetObjectByName("left_bg").visible = (style == "Left");  Widget.GetObjectByName("button_label").visible = (style == "Button");  Widget.GetObjectByName("button_bg").visible = (style == "Button");  Widget.GetObjectByName("right_label").textChars = values[0].value;  Widget.GetObjectByName("left_label").textChars = values[0].value;  Widget.GetObjectByName("button_label").textChars = values[0].value;}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}