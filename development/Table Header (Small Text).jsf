//==================================================================// iPhone Table Header (Small Text)// Timur Prokopiev 2011//==================================================================function setDefaultValues() {  var values = new Array();  // Header Text  values.push({ name:"Text", type:"text", value:"Small text" });Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  		Widget.GetObjectByName("table header").textChars = values[0].value;	}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}